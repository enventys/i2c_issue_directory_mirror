+  hspi->Init.DataSize = SPI_DATASIZE_8BIT;
+  hspi->Init.CLKPolarity = SPI_POLARITY_LOW;
+  hspi->Init.CLKPhase = SPI_PHASE_1EDGE;
+  hspi->Init.NSS = SPI_NSS_SOFT;
