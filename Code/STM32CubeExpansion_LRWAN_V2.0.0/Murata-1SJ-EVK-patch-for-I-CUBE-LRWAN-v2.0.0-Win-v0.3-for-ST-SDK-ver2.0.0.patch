+{
+  uint16_t rxData ;
+
+  /*RADIO_SPI_SendRecv(( uint8_t * ) &txData, ( uint8_t* ) &rxData, 1);*/
+  RADIO_SPI_SendRecv((uint8_t *) &txData, (uint8_t *) &rxData, 1);
