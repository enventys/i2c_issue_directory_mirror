+uint16_t BH1750_Dat_To_Lux(uint8_t* dat)
+{
+    uint16_t lux = 0;
+    lux = dat[0];
+    lux <<= 8;
+    lux += dat[1];
+    lux = (int)(lux / 1.2);
+
+    return lux;
+}
