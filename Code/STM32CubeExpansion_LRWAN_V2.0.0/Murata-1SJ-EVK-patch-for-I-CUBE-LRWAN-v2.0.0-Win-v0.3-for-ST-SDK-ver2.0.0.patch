+
+  Sx_Board_IoDeInit();
+
+  HAL_ADC_MspDeInit(&hadc1);
+  UTIL_ADV_TRACE_IoDeInit();//@Murata DeInit the trace io when enter stop mode
+
+  /*clear wake up flag*/
+  SET_BIT(PWR->CR, PWR_CR_CWUF);
+
+  UTILS_EXIT_CRITICAL_SECTION();
