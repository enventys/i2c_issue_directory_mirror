+static uint8_t Char2Nibble(char Char)
+{
+  if (((Char >= '0') && (Char <= '9')))
+  {
+    return Char - '0';
+  }
+  else if (((Char >= 'a') && (Char <= 'f')))
+  {
+    return Char - 'a' + 10;
+  }
+  else if ((Char >= 'A') && (Char <= 'F'))
+  {
+    return Char - 'A' + 10;
+  }
+  else
+  {
+    return 0xF0;
+  }
+  /* USER CODE BEGIN CertifSend_2 */
+
+  /* USER CODE END CertifSend_2 */
+}
